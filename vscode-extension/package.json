{
	"name": "vscode-snippets",
	"displayName": "My Snippets",
	"description": "VS Code snippets used in my work",
	"version": "1.0.0",
	"private": true,
	"publisher": "Knissing",
	"engines": {
		"vscode": "^1.42.0"
	},
	"categories": [
		"Snippets"
	],
	"activationEvents": [
		"onCommand:extension.helloWorld",
		"onCommand:extension.showDate"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.helloWorld",
				"title": "Hello World"
			},
			{
				"command": "extension.showDate",
				"title": "Show Current Date"
			}
		],
		"snippets": [
			{
				"language": "vue-html",
				"path": "./src/snippets/vue-html.json"
			},
			{
				"language": "vue",
				"path": "./src/snippets/vue.json"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.1",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.42.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.18.0",
		"@typescript-eslint/eslint-plugin": "^2.18.0",
		"glob": "^7.1.6",
		"mocha": "^10.2.0",
		"typescript": "^3.7.5",
		"vscode-test": "^1.3.0"
	},
	"author": "Knissing"
}
